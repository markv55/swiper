	<!-- startup action - load the first scene -->
	<action name="startup" autorun="onstart">
		if(startscene === null OR !scene[get(startscene)], copy(startscene,scene[0].name); );
		loadscene(get(startscene), null, MERGE);
		if(startactions !== null, startactions() );
		
		nextscene();
		maat_bepalen();
		knipper_logo();		
		
		
	</action>

<action name="nextscene"> 
	set(ns, get(scene[get(xml.scene)].index));
	set(maxs, get(scene.count));
	add(ns,1);
	if(ns == maxs, set(ns,0));
	loadscene(get(scene[get(ns)].name), null, KEEPMOVING, BLEND(1));
	delayedcall (20,nextscene());
</action>		
	
<action name="knipper_logo"> 
	logo_aan();
	delayedcall(2,logo_uit(); );	
	delayedcall (4,knipper_logo(); );	
</action>		
		
<action name="logo_aan"> 
set(layer[intro].visible,true)
</action>	

<action name="logo_uit"> 
set(layer[intro].visible,false)
</action>		
	
	
	
<action name="maat_bepalen">


if(stagewidth LT stageheight,  <!-- breedte kleiner dan hoogte =  portrait-->
 
 	<!-- PORTRAIT -->
	if (device.mobile, set (info_maat, calc(stagewidth*0.8));  );
	if (device.tablet, set (info_maat, calc(stagewidth*0.8));  );
 	,
  	<!-- LANDSCAPE -->
	if (device.mobile, set (info_maat, calc(stageheight*0.8));  );
	if (device.tablet, set (info_maat, calc(stageheight*0.8));  );
	
);


if (device.desktop, set (info_maat, calc(stageheight*0.5)); );

<!-- onderstaande wordt altijd uitgevoerd of hij nu hierboven wel of niet bij mobile, tablet of desktop is geweest -->

set (layer[intro].width, calc(info_maat*1)); set (layer[intro].height, calc(info_maat*1));


</action>	
	
	
	
<layer name="intro" url="foto/intro.png"  keep="true" visible="false" zorder="2" handcursor="false" capture="false"  align="center"  />

	
	<autorotate enabled="true" waittime="0" speed="20.0"  accel="30"    horizon="0"     tofov="160.0"    />  		
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
